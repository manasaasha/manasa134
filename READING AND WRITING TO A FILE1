#handlimg division by zero error
try:
  num1= int(input("enter a number:"))
  num2= int(input("enter a denominator:"))
  result= num1/num2
except ZeroDivisionError:
  print("cannot devide by zero")
except ValueError:
  print("invalid input!enter numeric values")    

