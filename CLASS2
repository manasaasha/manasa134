class ParentClass:
  def __init__ (self,name):
    self.name=name
  def name1(self):
    print(f"hello,my name is{self.name}")
class ChildClass(ParentClass):
  def __init__(self,name,age):
    super().__init__(name)
    self.age = age
  def age1(self):
    print(f"{self.age} years old")
child = ChildClass("Alice",12)
child.name1()
child.age1()
